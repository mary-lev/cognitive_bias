@prefix bias: <http://example.org/bias#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

# Classes
:Event a owl:Class .

:GeneralEvent a owl:Class ;
    rdfs:subClassOf :Event .

:SpecificEvent a owl:Class ;
    rdfs:subClassOf :GeneralEvent .

:Person a owl:Class .

:Judgment a owl:Class .

:RationalDecision a owl:Class ;
    rdfs:subClassOf :Judgment .

:BiasedDecision a owl:Class ;
    rdfs:subClassOf :Judgment .

# Properties
:hasDetail a owl:DatatypeProperty ;
    rdfs:domain :Event ;
    rdfs:range xsd:string .

:makesJudgment a owl:ObjectProperty ;
    rdfs:domain :Person ;
    rdfs:range :Judgment .

:basedOn a owl:ObjectProperty ;
    rdfs:domain :Judgment ;
    rdfs:range :Event .

:resultsIn a owl:ObjectProperty ;
    rdfs:domain :Judgment ;
    rdfs:range [ owl:unionOf (:RationalDecision :BiasedDecision) ] .
